{
  "expo": {
    "plugins": [
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true,
            "minifyEnabled": true,
            "shrinkResources": true,
            "enableProguardInReleaseBuilds": true,
            "packagingOptions": {
              "pickFirst": [
                "**/libc++_shared.so",
                "**/libjsc.so"
              ]
            }
          }
        }
      ]
    ],
    "name": "Quiz Supervisor",
    "slug": "quizSupervisor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-square-01.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "backgroundColor": "#1a1a1a",
    "splash": {
      "image": "./assets/logo-square-01.png",
      "resizeMode": "cover",
      "backgroundColor": "#1a1a1a",
      "dark": {
        "image": "./assets/logo-square-01.png",
        "resizeMode": "cover",
        "backgroundColor": "#1a1a1a"
      }
    },
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#1a1a1a",
      "splash": {
        "image": "./assets/splash-logo-dark.png",
        "resizeMode": "contain",
        "backgroundColor": "#1a1a1a",
        "tabletImage": "./assets/splash-logo-dark.png",
        "dark": {
          "image": "./assets/splash-logo-dark.png",
          "resizeMode": "contain",
          "backgroundColor": "#1a1a1a",
          "tabletImage": "./assets/splash-logo-dark.png"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-square-01.png",
        "backgroundColor": "#1a1a1a"
      },
      "backgroundColor": "#1a1a1a",
      "splash": {
        "image": "./assets/logo-square-01.png",
        "resizeMode": "cover",
        "backgroundColor": "#1a1a1a",
        "dark": {
          "image": "./assets/logo-square-01.png",
          "resizeMode": "cover",
          "backgroundColor": "#1a1a1a"
        }
      },
      "package": "com.andriiradchenko.quizPlayer",
      "versionCode": 3,
      "enableProguardInReleaseBuilds": true,
      "enableSeparateBuildPerCPUArchitecture": true
    },
    "web": {
      "favicon": "./assets/logo-square-01.png",
      "backgroundColor": "#1a1a1a"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0012d5c4-9b20-4830-9c5d-2d1bce7af760"
      }
    }
  }
}
